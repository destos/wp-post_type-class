Usage

NewPostType::instance()->add(array(
'post_type' => 'member',
'args' => array(
		'rewrite' => array( 'slug' => 'member' ),
		'supports' => array( 'title', 'editor', 'excerpt', 'thumbnail' ),
		'menu_icon' => get_bloginfo('template_directory').'/functions/classes/posttype/icons/zone.png'
	),
'thumbs' => array(
		'member-logo-small' => array( 80, 80, true ),
		'member-logo-large' => array( 250, 250, true )
	)
));